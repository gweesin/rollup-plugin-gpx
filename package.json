{
  "name": "rollup-plugin-gpx",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@10.6.2",
  "description": "A Vite/Rollup plugin to parse GPX files into GeoJSON format",
  "author": "gweesin<gweesin2000@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/gweesin/rollup-plugin-gpx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gweesin/rollup-plugin-gpx.git"
  },
  "bugs": "https://github.com/gweesin/rollup-plugin-gpx/issues",
  "keywords": [
    "vite",
    "plugin",
    "gpx",
    "parse",
    "gpx files"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json",
    "./gpx": {
      "types": "./gpx.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "gpx.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@tmcw/togeojson": "catalog:",
    "@types/xmldom": "catalog:",
    "geojson": "catalog:",
    "xmldom": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@antfu/ni": "catalog:cli",
    "@antfu/utils": "catalog:inlined",
    "@types/geojson": "catalog:",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "eslint": "catalog:cli",
    "lint-staged": "catalog:cli",
    "rollup": "catalog:",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:utils",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "unbuild": "catalog:cli",
    "vite": "catalog:cli",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
